{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "react-app",
        "prettier",
        // "eslint:recommended",
        // "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react","prettier"],
    "rules": {
        "extends": "standard",
        "react/react-in-jsx-scope": "off",
        "semi":[1, "always"],
        "no-console":"warn"

    }
}

